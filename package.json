    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-vue": "^9.33.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.5.3",
    "sass": "^1.89.1",
    "stylelint": "^16.20.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-order": "^7.0.0",
    "terser": "^5.29.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.5",
